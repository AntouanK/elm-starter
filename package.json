{
    "version": "0.0.8",
    "name": "elm-starter",
    "nameLong": "elm-starter - An Elm-based bootstrapper for Elm applications",
    "description": "An Elm-based light way to setup a fully fledged Elm PWA.",
    "author": "Luca Mugnaini",
    "twitterSite": "lucamug",
    "twitterCreator": "lucamug",
    "homepage": "https://elm-starter.guupa.com",
    "license": "BSD-3-Clause",
    "snapshotWidth": 700,
    "snapshotHeight": 350,
    "themeColor": {
        "red": 15,
        "green": 85,
        "blue": 123
    },
    "main": "index.js",
    "scripts": {
        "start": "node ./src-elm-starter/starter.js start",
        "build": "node ./src-elm-starter/starter.js build",
        "serverBuild": "node ./src-elm-starter/starter.js serverBuild",
        "elm-starter:boot": "node ./src-elm-starter/starter.js boot",
        "elm-starter:start": "node ./src-elm-starter/starter.js start",
        "elm-starter:generateDevFiles": "node ./src-elm-starter/starter.js generateDevFiles",
        "elm-starter:build": "node ./src-elm-starter/starter.js build",
        "elm-starter:buildExpectingTheServerRunning": "node ./src-elm-starter/starter.js buildExpectingTheServerRunning",
        "elm-starter:serverBuild": "node ./src-elm-starter/starter.js serverBuild",
        "elm-starter:serverDev": "node ./src-elm-starter/starter.js serverDev",
        "elm-starter:serverStatic": "node ./src-elm-starter/starter.js serverStatic",
        "elm-starter:watchStartElm": "node ./src-elm-starter/starter.js watchStartElm"
    },
    "devDependencies": {
        "chokidar-cli": "^2.1.0",
        "concurrently": "^5.2.0",
        "elm": "^0.19.1-3",
        "elm-live": "git://github.com/lucamug/elm-live.git#patch-4",
        "html-minifier": "^4.0.0",
        "puppeteer": "^4.0.1",
        "terser": "^4.8.0"
    },
    "dependencies": {
        "puppeteer-core": "^4.0.1"
    }
}
